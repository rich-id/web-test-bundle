<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="true" autoconfigure="true" public="false"/>

        <service id="RichCongress\WebTestBundle\Command\DebugOverridenServicesCommand" />
        <service id="RichCongress\WebTestBundle\OverrideService\OverrideServiceManager" />

        <service id="RichCongress\WebTestBundle\Doctrine\TestConnectionFactory"
                 decorates="dama.doctrine.dbal.connection_factory">
            <argument key="$decoratedFactory" type="service" id="RichCongress\WebTestBundle\Doctrine\TestConnectionFactory.inner" />
        </service>
    </services>
</container>
